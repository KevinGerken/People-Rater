<% include ../partials/header %>
<section>
  <form action="/users" method="post">
    <label>
      First Name
      <input type="text" placeholder="First Name" name="user[firstName]">
    </label>
    <label>
      Last Name
      <input type="text" placeholder="Last Name" name="user[lastName]">
    </label>
    <label>
      City
      <input type="text" placeholder="City" name="user[city]">
    </label>
    <label>
      State
      <input type="text" placeholder="State" name="user[state]">
    </label>
    <label>
      Image URL
      <input type="text" placeholder="Image URL" name="user[image]">
    </label>
    <label>
      Image Description
      <input type="text" placeholder="Image Description" name="user[imageAlt]">
    </label>
    <label>
      Username
      <input type="text" placeholder="Username" name="username">
    </label>
    <label>
      Password
      <input type="text" placeholder="Password" name="password">
    </label>
    <button class="only-button">Submit</button>
  </form>
</section>

<% include ../partials/footer %>