<% include ../partials/header %>

<form action="/users" method="post">
  <input type="text" placeholder="First Name" name="user[firstName]">
  <input type="text" placeholder="Last Name" name="user[lastName]">
  <input type="text" placeholder="City" name="user[city]">
  <input type="text" placeholder="State" name="user[state]">
  <input type="text" placeholder="Image" name="user[image]">
  <input type="text" placeholder="Image Description" name="user[imageAlt]">
  <input type="text" placeholder="Username" name="user[username]">
  <input type="text" placeholder="Password" name="password">
  <button>Submit</button>
</form>

<% include ../partials/footer %>